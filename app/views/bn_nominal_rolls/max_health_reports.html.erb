						<td>
							<%= ere.spg %>
						</td>
						<td>
							<%= ere.remarks %>
						</td>
						<td>
							<%= link_to "Edit", bn_nominal_roll_path(ere.bn_nominal_roll)%>
						</td>
					</tr>
				<% end %> 
			</tbody>
		</table>
	</div>
  <%= paginate @max_health_reports %>
</div>