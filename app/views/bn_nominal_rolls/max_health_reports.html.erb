						<th class="align-top">
							<%= sort_link(@q, :neutrophiles, 'NEUTROPHILS (40 - 80)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :lymphocytes, 'LYMPHOCYTES (20 - 40)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :monocytes, 'MONOCYTES (2 - 10)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :eosinophiles, 'EOSINOPHILS (1 - 6)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :basophiles, 'BASOPHILS (0 - 2)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :glucose, 'GLUCOSE (FASTING) (74 - 99)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :hbaglycosylated, 'HbA1C GLYCOSYLATED (<5.7)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :thyroid, 'THYROID (0.34 - 5.6)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :urea, 'UREA (17.0 - 43.0)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :creatinine, 'CREATININE (0.9 - 1.3)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :billirubin, 'BILLIRUBIN (0.3 - 1.2)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :sgot, 'SGOT (< 50)') %>
