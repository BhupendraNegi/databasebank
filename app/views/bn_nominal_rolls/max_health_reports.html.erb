						<th class="align-top">
							<%= sort_link(@q, :spg, 'SPG (1.015 - 1.025)') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :remarks, 'REMARKS') %>
						</th>
						<th>
						</th>
				</tr>
			</thead>
			<tbody>
				<% @max_health_reports.each do | ere |%>
					<tr>
						<td>
							<%= ere.bn_nominal_roll.army_no %>
						</td>
						<td>
							<%= ere.bn_nominal_roll.rank %>
