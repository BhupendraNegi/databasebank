							<%= ere.leucocyte %>
						</td>
						<td>
							<%= ere.neutrophiles %>
						</td>
						<td>
							<%= ere.lymphocytes %>
						</td>
						<td>
							<%= ere.monocytes %>
						</td>
						<td>
							<%= ere.eosinophiles %>
						</td>
						<td>
							<%= ere.basophiles %>
						</td>
						<td>
							<%= ere.glucose %>
						</td>
						<td>
							<%= ere.hbaglycosylated %>
						</td>
						<td>
							<%= ere.thyroid %>
						</td>
						<td>
							<%= ere.urea %>
						</td>
						<td>
							<%= ere.creatinine %>
						</td>
						<td>
							<%= ere.billirubin %>
						</td>
						<td>
							<%= ere.sgot %>
						</td>
						<td>
							<%= ere.sgpt %>
						</td>
						<td>
							<%= ere.cholesterol %>
						</td>
						<td>
							<%= ere.triglycerides %>
