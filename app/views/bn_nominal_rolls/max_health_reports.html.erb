<div class="container">
  <h5 class="mt-4"> BN FAMILY MEMBER DETAILS </h5>
  <br>
	<%= render partial: "search_health" %>
	<div class="table-responsive">
		<table class="table table-bordered">
			<thead class="bg-light">
				<tr>
						<th class="align-top">
							<%= sort_link(@q, 'bn_nominal_roll_army_no', 'ARMY NO') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, 'bn_nominal_roll_rank', 'RANK') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, 'bn_nominal_roll_name', 'NAME') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, 'bn_nominal_roll_coy', 'COY') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, 'bn_nominal_roll_trade', 'TRADE') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :barcode, 'BARCODE') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :ht, 'HT') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :wt, 'WT') %>
						</th>
						<th class="align-top">
							<%= sort_link(@q, :bp, 'BP') %>
